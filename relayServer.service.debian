[Unit]
Description=The relayServer daemon.
After=network.target

[Service]
User=nobody
Group=nogroup
Type=simple
StandardOutput=null
StandardError=null
Restart=no
ExecStart=/bin/bash -l -c '/usr/bin/node --expose_gc  /usr/local/share/relayServer/relayServer.js'

[Install]
WantedBy=multi-user.target
