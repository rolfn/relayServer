[Unit]
Description=The relayServer daemon.
After=network.target

[Service]
User=wwwrun
Group=www
Type=simple
StandardOutput=null
StandardError=null
Restart=always
ExecStart=/bin/bash -l -c "/usr/bin/node /usr/local/share/vaclab/nodejs/relayServer.js"
#         ^^^-- Ist dies die beste LÃ¶sung (Environment)?

[Install]
WantedBy=multi-user.target
