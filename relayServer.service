[Unit]
Description=The relayServer daemon.
Requires=network-online.target
After=network-online.target

[Service]
User=vaclab
Type=simple
#StandardOutput=null
#StandardError=null
# Load if exist:
EnvironmentFile=-/etc/environment
WorkingDirectory=/usr/local/share/vaclab/relayServer
Restart=on-failure
RestartSec=3
ExecStart=/bin/bash -l -c '/usr/bin/node ./relayServer.js'

[Install]
WantedBy=multi-user.target
